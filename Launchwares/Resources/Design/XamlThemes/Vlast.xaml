<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design.XamlThemes">
    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>

    <Brush x:Key="Primary">#FFCE00</Brush>
    <Brush x:Key="Secondary">#493C36</Brush>
    <Brush x:Key="Tertiary">#CFA707</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#FFCE00</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#493C36</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#CFA707</SolidColorBrush>
</ResourceDictionary>