<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design.XamlThemes">
    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>

    <Brush x:Key="Primary">#E15F1A</Brush>
    <Brush x:Key="Secondary">#542D18</Brush>
    <Brush x:Key="Tertiary">#FA691D</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#E15F1A</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#542D18</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#FA691D</SolidColorBrush>
</ResourceDictionary>