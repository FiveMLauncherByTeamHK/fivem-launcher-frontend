<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design.XamlThemes">
    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>

    <Brush x:Key="Primary">#AD3232</Brush>
    <Brush x:Key="Secondary">#7D0000</Brush>
    <Brush x:Key="Tertiary">#E83838</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#AD3232</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#7D0000</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#E83838</SolidColorBrush>
</ResourceDictionary>