<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design.XamlThemes">
    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>


    <Brush x:Key="Primary">#40ff40</Brush>
    <Brush x:Key="Secondary">#0f410f</Brush>
    <Brush x:Key="Tertiary">#111411</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#40ff40</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#0f410f</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#111411</SolidColorBrush>
</ResourceDictionary>