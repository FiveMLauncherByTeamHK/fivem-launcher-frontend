<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design.XamlThemes">
    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>

    <Brush x:Key="Primary">#312c7d</Brush>
    <Brush x:Key="Secondary">#181640</Brush>
    <Brush x:Key="Tertiary">#24215e</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#312c7d</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#181640</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#24215e</SolidColorBrush>
</ResourceDictionary>