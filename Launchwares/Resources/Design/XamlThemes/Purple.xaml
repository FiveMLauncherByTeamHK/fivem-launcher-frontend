<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design.XamlThemes">
    
    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>

    <Brush x:Key="Primary">#84596E</Brush>
    <Brush x:Key="Secondary">#302835</Brush>
    <Brush x:Key="Tertiary">#461b52</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#20639B</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#173F5F</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#461b52</SolidColorBrush>
</ResourceDictionary>