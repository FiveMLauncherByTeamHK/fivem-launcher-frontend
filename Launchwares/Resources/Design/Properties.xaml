<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Launchwares.Resources.Design"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <Brush x:Key="White">#ffffff</Brush>
    <Brush x:Key="Black">#000000</Brush>

    <Brush x:Key="Primary">#000000</Brush>
    <Brush x:Key="Secondary">#000000</Brush>
    <Brush x:Key="Tertiary">#303030</Brush>

    <SolidColorBrush x:Key="PrimarySolidColor">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="SecondarySolidColor">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="TertiarySolidColor">#303030</SolidColorBrush>

    <FontFamily x:Key="Cresta">/Fonts/#Cresta-Regular</FontFamily>
</ResourceDictionary>